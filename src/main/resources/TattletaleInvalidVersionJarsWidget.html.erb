<div class="yui-g">
	<div class="dashbox" style="height:100%">
		<p class="title">
			<%= message('widget.invalidversionjars.title') -%>
		</p>
		<% if measure('html_invalidversionjars') && measure('invalidversionjars') && measure('totaljars') %>
		<p>
			<span class="big"> <%= format_measure('invalidversionjars') -%> <%= tendency_icon(measure('invalidversionjars')) -%></span> /<%= format_measure('totaljars') -%>
		</p>
		<% else %>
			<br />
			<p>
				<%= message('widget.sonar-tattletale-plugin.error') -%>
			</p>
		<% end %>
		<script type="text/javascript">
		
			<% if measure('html_invalidversionjars') && measure('invalidversionjars') && measure('totaljars') %>
				function fillsInvalidVersionJars() {
					var invalidVersionHtmlData = '<%= measure('html_invalidversionjars').data -%>';
					document.getElementById('invalidVersionJarsIframe').contentDocument.body.innerHTML = invalidVersionHtmlData;
				}

				Event.observe(window, 'load', function() {
					fillsInvalidVersionJars();
				});
			<% end %>
		</script>
	</div>
</div>
<% if measure('html_invalidversionjars') && measure('invalidversionjars') && measure('totaljars') %> 
	<iframe id="invalidVersionJarsIframe" width="100%" style="border: 0"></iframe>
<% end %> 